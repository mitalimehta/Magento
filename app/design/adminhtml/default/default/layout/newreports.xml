        <reference name="content">
            <block type="newreports/adminhtml_salesreport_product" template="newreports/grid/container.phtml" name="adminhtml.salesreport.product" >
                <block type="adminhtml/store_switcher" template="report/store/switcher/enhanced.phtml" name="store.switcher">
                    <action method="setStoreVarName"><var_name>store_ids</var_name></action>
                </block>
                <block type="newreports/adminhtml_filter_form" name="adminhtml.salesreport.filter.form">
                    <action method="addReportTypeOption" translate="value">
                        <key>created_at_order</key>
                        <value>Created At Order</value>
                    </action>
                    <action method="addReportTypeOption" translate="value">
                        <key>updated_at_order</key>
